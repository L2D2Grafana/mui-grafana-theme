{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "rootDir": "./src",
    "baseUrl": "./src",
    "outDir": "./dist",
    "jsx": "react",
    "typeRoots": ["./node_modules/@types"],
    "skipLibCheck": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*", "@mui/material"],
  "exclude": ["./node_modules"]
}
